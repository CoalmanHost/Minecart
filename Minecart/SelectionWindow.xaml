<Window x:Class="Minecart.MinecartObjectsSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minecart"
        mc:Ignorable="d"
        Title="Select" Height="454" Width="341.6">
    <Grid>
        <ListBox x:Name="ObjectsListBox" HorizontalAlignment="Left" Height="367" Margin="10,10,0,0" VerticalAlignment="Top" Width="315" SelectionChanged="ObjectsListBox_SelectionChanged"/>
        <Button x:Name="ApplyButton" Content="Apply" HorizontalAlignment="Left" Height="23" Margin="206,392,0,0" VerticalAlignment="Top" Width="119" Click="ApplyButton_Click"/>
        <Button x:Name="AddButton" Content="Add" HorizontalAlignment="Left" Height="23" Margin="10,392,0,0" VerticalAlignment="Top" Width="121" Click="AddButton_Click"/>

    </Grid>
</Window>
